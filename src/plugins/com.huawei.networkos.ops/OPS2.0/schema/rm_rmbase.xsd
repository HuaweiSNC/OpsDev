		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="rm_rmbase_uniAf_type">
		<xs:annotation>
			<xs:appinfo>
				<autoDeleteChildren>true</autoDeleteChildren>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="vrfName">
				<xs:annotation>
					<xs:documentation>VPN实例的名称，其中公网的为public</xs:documentation>
					<xs:appinfo>
						<key>true</key>
						<access>readWrite</access>
						<mandatory>true</mandatory>
						<unique>true</unique>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="vrfName">
							<xs:attribute name="vrfId"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="afType" type="commodelAfType" default="ipv4unicast">
				<xs:annotation>
					<xs:documentation>地址族标识</xs:documentation>
					<xs:appinfo>
						<key>true</key>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="frrEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>是否使能FRR</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="routeLimitNum">
				<xs:annotation>
					<xs:documentation>VPN实例的路由数量限制</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="4294967295"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="simplyAlert" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>是否只是输出警告，与alertPercent互斥</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="alertPercent">
				<xs:annotation>
					<xs:documentation>输出警告时的百分比，与simplyAlert互斥</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
						<unit>%</unit>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="routeUnChanged" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>是否撤销超限的路由</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
