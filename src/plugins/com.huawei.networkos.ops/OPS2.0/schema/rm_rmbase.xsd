	<xs:complexType name="rm_rmbase_relayTunnel_type">
		<xs:annotation>
			<xs:documentation>路由迭代隧道</xs:documentation>
			<xs:appinfo/>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="relayEnable" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>路由迭代使能</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="prefixName">
				<xs:annotation>
					<xs:documentation>前缀过滤列表 （限制路由范围）</xs:documentation>
