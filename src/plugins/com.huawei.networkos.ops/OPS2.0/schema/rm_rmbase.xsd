						<xs:maxLength value="169"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tnlPolicyName">
				<xs:annotation>
					<xs:documentation>隧道策略名字</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
						<allowNull>true</allowNull>
