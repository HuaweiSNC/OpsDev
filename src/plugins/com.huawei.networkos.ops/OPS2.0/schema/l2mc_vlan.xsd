					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vlanId">
				<xs:annotation>
					<xs:documentation>VLAN的索引</xs:documentation>
