					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="snoopingEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>VLAN或VSI的igmp snooping使能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="groupAcl">
