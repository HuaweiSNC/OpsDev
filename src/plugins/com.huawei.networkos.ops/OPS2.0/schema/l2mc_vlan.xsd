					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="5"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="rtAgingTime" default="180">
				<xs:annotation>
					<xs:documentation>路由端口的存活时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="1000"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="rtPortLearning" type="xs:boolean" default="true">
				<xs:annotation>
					<xs:documentation>使能VLAN的路由器接口动态学习功能</xs:documentation>
