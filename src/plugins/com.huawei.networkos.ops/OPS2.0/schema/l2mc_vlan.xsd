				</xs:simpleType>
			</xs:element>
			<xs:element name="ssmapEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>接口SSMapping 使能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="fwdMode" type="l2mcFwdMode" default="ip">
				<xs:annotation>
					<xs:documentation>转发模式： 1:ip, 2:mac</xs:documentation>
