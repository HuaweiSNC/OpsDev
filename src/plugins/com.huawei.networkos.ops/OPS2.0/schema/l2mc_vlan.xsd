			<xs:element name="soureLifeTime" default="210">
				<xs:annotation>
					<xs:documentation>路由端口的存活时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="1000"/>
						<xs:minInclusive value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="l2McVlanDyn" type="l2mc_l2McVlanDyn_type"/>
			<xs:element name="l2McQuerier" type="l2mc_l2McQuerier_type"/>
			<xs:element name="l2McRoutePorts" type="l2mc_l2McRoutePorts_type"/>
			<xs:element name="l2McHostPorts" type="l2mc_l2McHostPorts_type"/>
			<xs:element name="l2McPortInfoEntrys" type="l2mc_l2McPortInfoEntrys_type"/>
			<xs:element name="l2McVlanStatis" type="l2mc_l2McVlanStatis_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVlanOthers_type">
