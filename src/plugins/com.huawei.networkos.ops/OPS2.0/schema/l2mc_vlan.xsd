						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="immeLeaveAcl">
				<xs:annotation>
					<xs:documentation>配置立刻离开组ACL Name or Number</xs:documentation>
