<xs:schema xmlns="http://www.huawei.com/netconf/vrp" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.huawei.com/netconf/vrp" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="en">
    <xs:annotation>
        <xs:appinfo>
            <formatVersion>1.0</formatVersion>
            <contentVersion>1.0</contentVersion>
        </xs:appinfo>
    </xs:annotation>
    <xs:include schemaLocation="pub_attribute.xsd"/>
    <xs:include schemaLocation="pub_type.xsd"/>
    <xs:include schemaLocation="snmp_type.xsd"/>
    <xs:element name="snmp" type="snmp_snmp_type">
        <xs:annotation>
            <xs:appinfo>
                <taskName>snmp</taskName>
            </xs:appinfo>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="snmp_snmp_type">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="agentCfg" type="snmp_agentCfg_type"/>
            <xs:element name="engine" type="snmp_engine_type"/>
            <xs:element name="systemCfg" type="snmp_systemCfg_type"/>
            <xs:element name="notificationCfg" type="snmp_notificationLogCfg_type"/>
            <xs:element name="mibViews" type="snmp_mibViews_type"/>
            <xs:element name="communitys" type="snmp_communitys_type"/>
            <xs:element name="snmpv3Groups" type="snmp_snmpv3Groups_type"/>
            <xs:element name="usmUsers" type="snmp_usmUsers_type"/>
            <xs:element name="targetHosts" type="snmp_targetHosts_type"/>
            <xs:element name="notifyFltrPrs" type="snmp_notifyFltrPrs_type"/>
            <xs:element name="snmpStats" type="snmp_snmpStats_type"/>
            <xs:element name="usmStats" type="snmp_usmStats_type"/>
            <xs:element name="mpdStats" type="snmp_mpdStats_type"/>
            <xs:element name="notificationStats" type="snmp_notificationLogStats_type"/>
            <xs:element name="targetStats" type="snmp_targetStats_type"/>
            <!--<xs:element name="proxy" type="snmp_proxy_type"/>-->
        </xs:choice>
        <xs:attributeGroup ref="versionAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_agentCfg_type">
        <xs:annotation>
            <xs:appinfo/>
            <xs:documentation>To configure SNMP Agent parameters</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="agentEnable" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>To enable or disable SNMP agent</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_engine_type">
        <xs:annotation>
            <xs:appinfo/>
            <xs:documentation>To configure SNMP engine parameters</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="version" type="snmpSnmpVersionE" default="v3">
                <xs:annotation>
                    <xs:documentation>SNMP versions</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="engineID">
                <xs:annotation>
                    <xs:documentation>SNMP Agent's engine ID</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="64"/>
                        <xs:minLength value="10"/>
                        <xs:pattern value="(([0-9a-fA-F])*)"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="engineMaxMessageSize" default="12000">
                <xs:annotation>
                    <xs:documentation>SNMP engine maximum message size</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:minInclusive value="484"/>
                        <xs:maxInclusive value="17940"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_systemCfg_type">
        <xs:annotation>
            <xs:appinfo/>
            <xs:documentation>To configure SNMP parameters</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="trapSourceInterfaceName" type="ifName">
                <xs:annotation>
                    <xs:documentation>Trap source interface</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="informTimeout" default="15">
                <xs:annotation>
                    <xs:documentation>Timeout period of SNMP inform packets (in seconds)</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="1800"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="informResendTimes" default="3">
                <xs:annotation>
                    <xs:documentation>Retransmission times of SNMP inform packets</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="informPendNumber" default="39">
                <xs:annotation>
                    <xs:documentation>Number of SNMP inform packets that are waiting for the inform ack from the NMS</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="2048"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="snmpPacketPriority" default="6">
                <xs:annotation>
                    <xs:documentation>Packet priority of SNMP response packets</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="7"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="trapPacketPriority" default="6">
                <xs:annotation>
                    <xs:documentation>Packet priority of SNMP trap packets</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="7"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_notificationLogCfg_type">
        <xs:annotation>
            <xs:appinfo/>
            <xs:documentation>To configure SNMP notification log parameters</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="notificationLogEnable" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>To enable or disable notification logging feature</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="notificationGlobalAgeOut" default="24">
                <xs:annotation>
                    <xs:documentation>The notification aging time in hours</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="36"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="notificationGlobalLimit" default="500">
                <xs:annotation>
                    <xs:documentation>Maximum number of notification entries to be stored</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="15000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_targetHosts_type">
        <xs:annotation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="targetHost" type="snmp_targetHost_type" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="snmp_targetHost_type">
        <xs:annotation>
            <xs:documentation>To configure target host for traps and informs</xs:documentation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="nmsName">
                <xs:annotation>
                    <xs:documentation>Unique name to identify target host entry</xs:documentation>
                    <xs:appinfo>
                        <key>true</key>
                        <access>readCreate</access>
                        <unique>true</unique>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="domain" type="snmpSnmpDomainE" default="snmpUDPDomain">
                <xs:annotation>
                    <xs:documentation>Transport domain</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="address">
                <xs:annotation>
                    <xs:documentation>Target host IP address</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="ipAddress">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="targetInformTimeout">
                <xs:annotation>
                    <xs:documentation>Timeout period of SNMP inform packets (in seconds) for the target</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="1800"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="targetInformResendTimes">
                <xs:annotation>
                    <xs:documentation>Maximum number of time to re-transmit the SNMP inform packet if acknowledgement is not received from the target</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="notifyType" type="snmpSnmpNotifyE" default="trap">
                <xs:annotation>
                    <xs:documentation>Trap or inform</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="vpnInstanceName" type="vrfName">
                <xs:annotation>
                    <xs:documentation>To configure VPN instance</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="portNumber" default="162">
                <xs:annotation>
                    <xs:documentation>To configure UDP port number</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="65535"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="securityModel" type="snmpSnmpTgtVerE" default="v1">
                <xs:annotation>
                    <xs:documentation>SNMP security model</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="securityName">
                <xs:annotation>
                    <xs:documentation>Security name</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="passwordExtend">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="securityLevel" type="snmpSnmpSecLvlE" default="noAuthNoPriv">
                <xs:annotation>
                    <xs:documentation>SNMP security level</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="private-netmanager" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>To enable or disable Private VB's for target Host</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="ext-vb" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>To enable or disable the extended VB's for target Host</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
        <xs:element name="notifyFltrPrName">
            <xs:annotation>
                <xs:documentation>Notify filter profile name</xs:documentation>
                <xs:appinfo>
                    <access>readCreate</access>
                    <allowNull>true</allowNull>
                </xs:appinfo>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                    <xs:maxLength value="32"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:element>
            <xs:element name="retriesNum" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>Number of retries made in sending inform</xs:documentation>
                    <xs:appinfo>
                        <access>readOnly</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingNum" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>Number of informs which are waiting for acknowledgement</xs:documentation>
                    <xs:appinfo>
                        <access>readOnly</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="sentNum" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>Number of packets sent</xs:documentation>
                    <xs:appinfo>
                        <access>readOnly</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="droppedNum" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>Number of packets dropped while sending</xs:documentation>
                    <xs:appinfo>
                        <access>readOnly</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="failedNum" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>Number of informs failed to receive acknowledgement</xs:documentation>
                    <xs:appinfo>
                        <access>readOnly</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="confirmedNum" type="xs:unsignedInt">
                <xs:annotation>
                    <xs:documentation>Number of informs received acknowledgement</xs:documentation>
                    <xs:appinfo>
                        <access>readOnly</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="interface-name" type="ifName">
                <xs:annotation>
                    <xs:documentation>Set the source interface of trap packet</xs:documentation>
                    <xs:appinfo>
                        <access>readWrite</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_communitys_type">
        <xs:annotation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="community" type="snmp_community_type" minOccurs="0" maxOccurs="25"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="snmp_community_type">
        <xs:annotation>
            <xs:documentation>To configure SNMP communtiy for SNMPv1 and SNMPv2</xs:documentation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="communityName">
                <xs:annotation>
                    <xs:documentation>Unique name to identify the community</xs:documentation>
                    <xs:appinfo>
                        <key>true</key>
                        <access>readCreate</access>
                        <unique>true</unique>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
					<xs:restriction base="passwordExtend">
                        <xs:maxLength value="32"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="accessRight" type="snmpSnmpAccessE">
                <xs:annotation>
                    <xs:documentation>Access right (read / write)</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="mibViewName">
                <xs:annotation>
                    <xs:documentation>Mib view name</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="aclNumber">
                <xs:annotation>
                    <xs:documentation>ACL</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_usmUsers_type">
        <xs:annotation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="usmUser" type="snmp_usmUser_type" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="snmp_usmUser_type">
        <xs:annotation>
            <xs:documentation>To configure USM users for SNMPv3</xs:documentation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="userName">
                <xs:annotation>
                    <xs:documentation>Unique name to identify the USM user</xs:documentation>
                    <xs:appinfo>
                        <key>true</key>
                        <access>readCreate</access>
                        <unique>true</unique>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="groupName">
                <xs:annotation>
                    <xs:documentation>Name of the group where user belongs to</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="authProtocol" type="snmpSnmpAuthenmodeE" default="noAuth">
                <xs:annotation>
                    <xs:documentation>Authentication protocol</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="authKey">
                <xs:annotation>
                    <xs:documentation>Authentication key</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="8"/>
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="privProtocol" type="snmpSnmpPrivacyE" default="noPriv">
                <xs:annotation>
                    <xs:documentation>Privacy protocol</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="privKey">
                <xs:annotation>
                    <xs:documentation>Privacy key</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                        <mandatory>true</mandatory>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="8"/>
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="aclNumber">
                <xs:annotation>
                    <xs:documentation>ACL</xs:documentation>
                    <xs:appinfo>
                        <access>readCreate</access>
                        <allowNull>true</allowNull>
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:choice>
        <xs:attributeGroup ref="objectAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="snmp_snmpv3Groups_type">
        <xs:annotation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="snmpv3Group" type="snmp_snmpv3Group_type" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="snmp_snmpv3Group_type">
        <xs:annotation>
            <xs:documentation>To configure SNMPv3 groups</xs:documentation>
            <xs:appinfo/>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="groupName">
                <xs:annotation>
                    <xs:documentation>Unique name to identify the SNMPv3 group</xs:documentation>
