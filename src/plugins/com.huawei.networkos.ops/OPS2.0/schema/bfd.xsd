<xs:schema xmlns="http://www.huawei.com/netconf/vrp" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.huawei.com/netconf/vrp" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="en">
	<xs:annotation>
		<xs:appinfo>
			<formatVersion>1.0</formatVersion>
			<contentVersion>1.0</contentVersion>
		</xs:appinfo>
	</xs:annotation>
	<xs:include schemaLocation="pub_attribute.xsd"/>
	<xs:include schemaLocation="pub_type.xsd"/>
	<xs:include schemaLocation="bfd_type.xsd"/>
	<xs:element name="bfd" type="bfd_bfd_type">
		<xs:annotation>
			<xs:appinfo>
				<taskName>bfd</taskName>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bfd_bfd_type">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="bfdSchGlobal" type="bfd_bfdSchGlobal_type"/>
			<xs:element name="bfdStat" type="bfd_bfdStat_type"/>
			<xs:element name="bfdCfgSessions" type="bfd_bfdCfgSessions_type"/>
			<xs:element name="bfdAllSessions" type="bfd_bfdAllSessions_type"/>
			<xs:element name="bindInterfaces" type="bfd_bindInterfaces_type"/>
		</xs:choice>
		<xs:attributeGroup ref="versionAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="bfd_bfdSchGlobal_type">
		<xs:annotation>
			<xs:appinfo/>
			<xs:documentation>BFD组件全局配置信息</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="bfdEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>BFD组件使能标志</xs:documentation>
					<xs:appinfo>
						<!--<key>true</key>-->
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<mandatory>true</mandatory>
						<unique>true</unique>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="delayUpTimer">
				<xs:annotation>
					<xs:documentation>延迟UP的时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="600"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="mplsPassive" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>是否配置mpls passive</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pingInterval" default="60">
				<xs:annotation>
					<xs:documentation>发送LSP Ping报文的时间间隔</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="30"/>
						<xs:maxInclusive value="600"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="processMode" type="bfdDeployMode">
				<xs:annotation>
					<xs:documentation>BFD部署方式:集中式,分布式</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="procCentralSlt">
				<xs:annotation>
					<xs:documentation>主集中处理板槽位号</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="19"/>
						<xs:minLength value="1"/>
						<xs:pattern value="(^\w{0,3}\d{0,2}(\/\d{1,2}){0,1}$)"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="procCentralSlt2">
				<xs:annotation>
					<xs:documentation>主集中处理板2槽位号</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="19"/>
						<xs:minLength value="1"/>
						<xs:pattern value="(^\w{0,3}\d{0,2}(\/\d{1,2}){0,1}$)"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="defaultIp">
				<xs:annotation>
					<xs:documentation>默认组播IPv4地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ipv4Address">
						<xs:maxLength value="11"/>
						<xs:minLength value="11"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tosExp" default="7">
				<xs:annotation>
					<xs:documentation>动态BFD会话报文优先级</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="bfd_bfdCfgSessions_type">
		<xs:annotation>
			<xs:appinfo/>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bfdCfgSession" type="bfd_bfdCfgSession_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="bfd_bfdCfgSession_type">
		<xs:annotation>
			<xs:documentation>BFD会话配置信息(VIST中的值maxoccurs项为PAF规格)</xs:documentation>
			<xs:appinfo/>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="sessName">
				<xs:annotation>
					<xs:documentation>会话名称</xs:documentation>
					<xs:appinfo>
						<key>true</key>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<mandatory>true</mandatory>
						<unique>true</unique>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
						<xs:minLength value="1"/>
						<xs:pattern value="(^[^\s?]*$)"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="createType" type="bfdCreateType" default="SESS_STATIC">
				<xs:annotation>
					<xs:documentation>BFD会话创建方式</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="localDiscr">
				<xs:annotation>
					<xs:documentation>本地标识符</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="16384"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="remoteDiscr">
				<xs:annotation>
					<xs:documentation>远端标识符</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="4294967295"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="linkType" type="bfdLinkType" default="IP">
				<xs:annotation>
					<xs:documentation>BFD检测的链路类型</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="addrType" type="bfdSmpBfdCap" default="IPV4">
				<xs:annotation>
					<xs:documentation>IP地址类型</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="destAddr">
				<xs:annotation>
					<xs:documentation>目的IP地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ipAddress">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="srcAddr">
				<xs:annotation>
					<xs:documentation>源IP地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ipAddress">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="nextHop">
				<xs:annotation>
					<xs:documentation>下一跳IP地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ipAddress">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="outIfName" type="ifName">
				<xs:annotation>
					<xs:documentation>出接口名称</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vrfName" type="vrfName">
				<xs:annotation>
					<xs:documentation>BFD所属VRF实例名</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tunnelName" type="ifName">
				<xs:annotation>
					<xs:documentation>绑定mpls隧道名称</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="teBackup" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>TE-LSP链路主备标记-0为主链路1为备链路</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="remotePeer">
				<xs:annotation>
					<xs:documentation>pw/vsi remote IP</xs:documentation>
					<xs:appinfo>
						<supportFilter>false</supportFilter>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ipAddress">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="autoCalc" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>自动计算ttl</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pwTtl">
				<xs:annotation>
					<xs:documentation>配置TTL,用于穿越SPE节点的PW,区分会话中的TTL</xs:documentation>
					<xs:appinfo>
						<supportFilter>false</supportFilter>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="pwSecondary" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>PW主备标示</xs:documentation>
					<xs:appinfo>
						<supportFilter>false</supportFilter>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vsiName">
				<xs:annotation>
					<xs:documentation>vsi name</xs:documentation>
					<xs:appinfo>
						<supportFilter>false</supportFilter>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="31"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="peerIp">
				<xs:annotation>
					<xs:documentation>Bfd for pw, bfd远端检测ip</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ipAddress">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="vcId">
				<xs:annotation>
					<xs:documentation>会话使用默认组播地址used default ip</xs:documentation>
					<xs:appinfo>
						<supportFilter>false</supportFilter>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="4294967295"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="minTxInt">
				<xs:annotation>
					<xs:documentation>最小发送报文的间隔</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="3"/>
						<xs:maxInclusive value="1000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="minRxInt">
				<xs:annotation>
					<xs:documentation>最小接收报文的间隔</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="3"/>
						<xs:maxInclusive value="1000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="detectMulti" default="3">
				<xs:annotation>
					<xs:documentation>检测倍数</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="3"/>
						<xs:maxInclusive value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="wtrTimerInt">
				<xs:annotation>
					<xs:documentation>wtr定时器超时间隔(1-60min)</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tosExp" default="7">
				<xs:annotation>
					<xs:documentation>报文优先级</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="adminDown" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>管理Down</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="description">
				<xs:annotation>
					<xs:documentation>会话描述信息</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="51"/>
						<xs:minLength value="1"/>
						<xs:pattern value="(^[^?]+(\s)*[^\s]+(\s)*$)"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="processPst" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>设置Process PST标记(FEI 微码检测到Down之后是否改PST表)</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pis" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>process-interface-status接口联动</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pisSubIf" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>pis子接口联动</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="useDefaultIp" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>会话使用默认组播地址used default ip</xs:documentation>
					<xs:appinfo>
						<supportFilter>false</supportFilter>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="procSlotId">
				<xs:annotation>
					<xs:documentation>强制选板槽位号</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<supportFilter>false</supportFilter>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bfdSessRunning" type="bfd_bfdSessRunning_type"/>
			<xs:element name="bfdSessStat" type="bfd_bfdSessStat_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="bfd_bfdSessRunnings_type">
		<xs:annotation>
			<xs:appinfo/>
		</xs:annotation>
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="bfd_bfdSessRunning_type">
