						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="512"/>
						<xs:maxInclusive value="16384"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="level2lspLengthOrig" default="1497">
				<xs:annotation>
					<xs:documentation>Level2本地生成的LSP报文size的最大值</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="512"/>
						<xs:maxInclusive value="16384"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lspReceiveLength" default="1497">
