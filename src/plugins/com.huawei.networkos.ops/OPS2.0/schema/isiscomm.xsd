					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lspRefreshInterval" default="900">
				<xs:annotation>
					<xs:documentation>LSP刷新周期</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="65534"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="shutDownEnable" type="xs:boolean" default="false">
