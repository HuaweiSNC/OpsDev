					<xs:documentation>接口开销自动计算功能中所使用的带宽参考值</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="2147483648"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="stdLevel1Cost" default="10">
				<xs:annotation>
					<xs:documentation>全局接口Level1Cost值</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="16777215"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="stdLevel2Cost" default="10">
				<xs:annotation>
					<xs:documentation>全局接口Level2Cost值</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="16777215"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="baseTopoType" type="isiscommBaseTopoType" default="invalid">
				<xs:annotation>
					<xs:documentation>使能IPv6的类型</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="description">
				<xs:annotation>
					<xs:documentation>description信息内容</xs:documentation>
					<xs:appinfo>
						<key>false</key>
						<mandatory>true</mandatory>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
						<xs:pattern value="([^?]*)"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isLspGenIntelliTimer" type="isiscomm_isLspGenIntelliTimers_type"/>
			<xs:element name="isFlashFlood" type="isiscomm_isFlashFloods_type"/>
			<xs:element name="isOverloadSet" type="isiscomm_isOverloadSets_type"/>
			<xs:element name="isLdpSyncdSet" type="isiscomm_isLdpSyncSets_type"/>
			<xs:element name="isLocalMtSets" type="isiscomm_isLocalMtSets_type"/>
			<xs:element name="isNetEntitys" type="isiscomm_isNetEntitys_type"/>
			<xs:element name="isLspAuthtications" type="isiscomm_isLspAuthtications_type"/>
			<xs:element name="isNameTables" type="isiscomm_isNameTables_type"/>
			<xs:element name="isVirtualSystems" type="isiscomm_isVirtualSystems_type"/>
			<xs:element name="isCircuits" type="isiscomm_isCircuits_type"/>
			<xs:element name="isSiteMTs" type="isiscomm_isSiteMTs_type"/>
			<xs:element name="isDispLsdbs" type="isiscomm_isDispLsdbs_type"/>
			<xs:element name="isDispPeers" type="isiscomm_isDispPeers_type"/>
			<xs:element name="isDispLspStatisticss" type="isiscomm_isDispLspStatisticss_type"/>
			<xs:element name="isDispBfdSessions" type="isiscomm_isDispBfdSessions_type"/>
			<xs:element name="isDispNameTables" type="isiscomm_isDispNameTables_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
