			<xs:element name="stdOverloadType" type="isiscommIsOverLoadType" default="no_set">
				<xs:annotation>
					<xs:documentation>用户是否设置过载位标志位</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="stdOverloadWaitType" type="isiscommWaitType" default="no_wait">
				<xs:annotation>
					<xs:documentation>过载类型</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="stdOverloadNbrSysId">
				<xs:annotation>
					<xs:documentation>邻居systemID</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="15"/>
						<xs:pattern value="^((([0-9]|[a-f]|[A-F]){4}\.){2}([0-9]|[a-f]|[A-F]){4})$"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="stdOverloadTimeout1" default="1200">
				<xs:annotation>
					<xs:documentation>定时器1</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
