			<xs:element name="level2LspFragExtEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>Level2LSP分片设置标志位</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
