						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="costStyle" type="isiscommCostStyle" default="narrow">
				<xs:annotation>
					<xs:documentation>ISIS开销值类型</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="relaxSpfLimit" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>允许接受开销值大于Narrow限制的路由</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="spfMaxInterval" default="5">
				<xs:annotation>
					<xs:documentation>路由计算最大延迟时间</xs:documentation>
