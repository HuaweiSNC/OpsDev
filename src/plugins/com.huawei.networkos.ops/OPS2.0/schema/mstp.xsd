<?xml version="1.0"?>
<xs:schema xmlns="http://www.huawei.com/netconf/vrp" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.huawei.com/netconf/vrp" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="en">
	<xs:include schemaLocation="pub_attribute.xsd"/>
	<xs:include schemaLocation="pub_type.xsd"/>
	<xs:include schemaLocation="mstp_type.xsd"/>
	<xs:annotation>
		<xs:appinfo>
			<formatVersion>1.0</formatVersion>
			<contentVersion>1.0</contentVersion>
		</xs:appinfo>
	</xs:annotation>
	<xs:element name="mstp" type="mstp_type">
		<xs:annotation>
			<xs:appinfo>
				<taskName>mstp</taskName>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="mstp_type">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="global" type="mstp_global_type"/>
			<xs:element name="processs" type="mstp_processs_type"/>
			<xs:element name="ports" type="mstp_ports_type"/>
			<xs:element name="errorPktInfos" type="mstp_errorPktInfos_type"/>
		</xs:choice>
		<xs:attributeGroup ref="versionAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_global_type">
		<xs:annotation>
			<xs:documentation/>
			<xs:appinfo>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="bpduFilterDef" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation/>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="edgePortDef" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation/>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_processs_type">
		<xs:sequence>
			<xs:element name="process" type="mstp_process_type" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mstp_process_type">
		<xs:annotation>
			<xs:documentation>进程</xs:documentation>
			<xs:appinfo>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="processId" default="0">
				<xs:annotation>
					<xs:documentation>MSTP进程ID</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="256"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="enable" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>进程使能开关</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="bpduProtection" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>BPDU保护功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="forwardDelay" default="1500">
				<xs:annotation>
					<xs:documentation>配置Forward Delay时间，单位厘秒，步长为100</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:maxInclusive value="3000"/>
						<xs:minInclusive value="400"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="helloTime" default="200">
				<xs:annotation>
					<xs:documentation>配置Hello Time时间，单位厘秒，步长为100</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:maxInclusive value="1000"/>
						<xs:minInclusive value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="maxAge" default="2000">
				<xs:annotation>
					<xs:documentation>配置Max Age时间，单位厘秒，步长为100</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:maxInclusive value="4000"/>
						<xs:minInclusive value="600"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="maxHops" default="20">
				<xs:annotation>
					<xs:documentation>配置MST域内生成树的最大跳数</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:maxInclusive value="40"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="mode" type="stpMode" default="Mstp">
				<xs:annotation>
					<xs:documentation>配置MSTP进程的工作模式</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pathCostStandard" type="stpPathCostStanard" default="Dot1T">
				<xs:annotation>
					<xs:documentation>配置路径开销缺省值的计算方法</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tcNotify" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>使能当前MSTP进程的TC通告功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tcProtection" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>使能MSTP进程对TC类型BPDU报文的保护功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tcProtectionThreshold" default="1">
				<xs:annotation>
					<xs:documentation>配置MSTP进程在收到TC类型BPDU报文后，单位时间内，处理TC类型BPDU报文并立即刷新转发表项的阈值</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="255"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="timerFactor" default="3">
				<xs:annotation>
					<xs:documentation>设定HelloTime的倍数（TimerFactor）</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedByte">
						<xs:maxInclusive value="10"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="linkShareProtect" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>用来启动MSTP进程的共享链路保护功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="regionName">
				<xs:annotation>
					<xs:documentation>配置MST域名，缺省为系统MAC地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<allowNull>true</allowNull>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
						<xs:minLength value="1"/>
						<xs:pattern value="^[^\s\?]*$"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="revisionLevel" default="0">
				<xs:annotation>
					<xs:documentation>配置MSTP修订级别</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:maxInclusive value="65535"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="syncFlag" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>域配置同步标记</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="regionInfo" type="mstp_regionInfo_type"/>
			<xs:element name="processInfo" type="mstp_processInfo_type"/>
			<xs:element name="processInstances" type="mstp_processInstances_type"/>
			<xs:element name="processInstanceInfos" type="mstp_processInstanceInfos_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_ports_type">
		<xs:sequence>
			<xs:element name="port" type="mstp_port_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mstp_port_type">
		<xs:annotation>
			<xs:documentation>端口数据</xs:documentation>
			<xs:appinfo>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="ifName">
				<xs:annotation>
					<xs:documentation>接口名</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="ifName">
							<xs:attribute name="ifIndex" type="xs:unsignedInt"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="enable" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>端口协议使能开关</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="bpduFilter" type="stpBpduFilter" default="Undo">
				<xs:annotation>
					<xs:documentation>BPDU过滤功能开关</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="compliance" type="stpCompliance" default="Auto">
				<xs:annotation>
					<xs:documentation>MSTP报文协议格式</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="configDigestSnoop" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>配置摘要侦听</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="edgePort" type="stpEdgePort" default="Default">
				<xs:annotation>
					<xs:documentation>边缘端口</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="loopProtection" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>环路保护</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="noAgreementCheck" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>增强快速迁移机制</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pointToPoint" type="stpPointToPoint" default="Auto">
				<xs:annotation>
					<xs:documentation>指定端口的链路类型</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="rootProtection" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>根保护</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="transmitLimit" default="147">
				<xs:annotation>
					<xs:documentation>Hello Time时间内发送BPDU的最大数目</xs:documentation>
					<xs:appinfo>
						<access>readWrite</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:maxInclusive value="255"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="processInstancePorts" type="mstp_processInstancePorts_type"/>
			<xs:element name="processPorts" type="mstp_processPorts_type"/>
			<xs:element name="portProcesss" type="mstp_portProcesss_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_errorPktInfos_type">
		<xs:sequence>
			<xs:element name="errorPktInfo" type="mstp_errorPktInfo_type" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mstp_errorPktInfo_type">
		<xs:annotation>
			<xs:documentation>错误报文统计信息</xs:documentation>
			<xs:appinfo>
				<access>readWrite</access>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="errorPktTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>最近错误报文的接收时间</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="errorPktCount" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>接收错误报文个数</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
						<supportFilter>false</supportFilter>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="errorPktContent">
				<xs:annotation>
					<xs:documentation>错误报文内容</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4500"/>
						<xs:minLength value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_processInstances_type">
		<xs:sequence>
			<xs:element name="processInstance" type="mstp_processInstance_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mstp_processInstance_type">
		<xs:annotation>
			<xs:documentation>进程实例配置，不依赖与实例是否创建，priorty和root互斥，需要通过linkage实现</xs:documentation>
			<xs:appinfo>
				<access>readCreate</access>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="instanceId" default="0">
				<xs:annotation>
					<xs:documentation>实例ID</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="48"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="priority" default="32768">
				<xs:annotation>
					<xs:documentation>实例优先级，步长为4096</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="61440"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="root" type="stpRoot" default="Normal">
				<xs:annotation>
					<xs:documentation>指定实例的根类型</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_processInstanceInfos_type">
		<xs:sequence>
			<xs:element name="processInstanceInfo" type="mstp_processInstanceInfo_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mstp_processInstanceInfo_type">
		<xs:annotation>
			<xs:documentation>进程下可见的实例信息，stp去使能时无数据</xs:documentation>
			<xs:appinfo>
				<access>readOnly</access>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="instanceId">
				<xs:annotation>
					<xs:documentation/>
					<xs:appinfo>
						<access>readOnly</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="48"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="cistInfos" type="mstp_cistInfos_type"/>
			<xs:element name="mstiInfos" type="mstp_mstiInfos_type"/>
			<xs:element name="mstiPortInfos" type="mstp_mstiPortInfos_type"/>
			<xs:element name="cistPortInfos" type="mstp_cistPortInfos_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="mstp_cistPortInfos_type">
		<xs:sequence>
			<xs:element name="cistPortInfo" type="mstp_cistPortInfo_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="mstp_cistPortInfo_type">
		<xs:annotation>
			<xs:documentation>CIST端口信息</xs:documentation>
			<xs:appinfo>
				<access>readOnly</access>
				<status>current</status>
				<version>V8R5</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="tcOrTcnSend" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>发送的TC或者TCN报文数量</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="tcOrTcnReceive" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>收到的TC或者TCN报文数量</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="txTcn" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>发送的TCN报文数量</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="txConf" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>发送的config报文数量</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="txRst" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>发送的RSTP报文数量</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="txMst" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>发送的MSTP报文数量</xs:documentation>
					<xs:appinfo>
						<access>readOnly</access>
					</xs:appinfo>
