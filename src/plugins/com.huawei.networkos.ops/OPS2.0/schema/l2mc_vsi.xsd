<?xml version="1.0"?>
<xs:schema xmlns="http://www.huawei.com/netconf/vrp" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.huawei.com/netconf/vrp" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="en">
	<xs:include schemaLocation="pub_attribute.xsd"/>
	<xs:include schemaLocation="pub_type.xsd"/>
	<xs:include schemaLocation="l2mc_type.xsd"/>
	<xs:annotation>
		<xs:appinfo>
			<formatVersion>1.0</formatVersion>
			<contentVersion>1.0</contentVersion>
		</xs:appinfo>
	</xs:annotation>
	<xs:complexType name="l2mc_l2McVsis_type">
		<xs:sequence>
			<xs:element name="l2McVsiCfgs" type="l2mc_l2McVsiCfgs_type"/>
			<xs:element name="l2McVsiOthers" type="l2mc_l2McVsiOthers_type">
				<xs:annotation>
					<xs:appinfo>
						<access>readWrite</access>
						<owner>product</owner>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="l2McVsiFastChannels" type="l2mc_l2McVsiFastChannels_type"/>
			<xs:element name="l2McVsiSSMs" type="l2mc_l2McVsiSSMs_type"/>
			<xs:element name="l2McForwardTlbVsiStatiss" type="l2mc_l2McForwardTlbVsiStatiss_type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiCfgs_type">
		<xs:sequence>
			<xs:element name="l2McVsiCfg" type="l2mc_l2McVsiCfg_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiDyns_type">
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiCfg_type">
		<xs:annotation>
			<xs:documentation>igmp snooping VSI参数配置和查询</xs:documentation>
			<xs:appinfo>
				<access>readCreate</access>
				<key>vsiName</key>
				<status>current</status>
				<version>1.0</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="addrFamily" type="l2mcAfType">
				<xs:annotation>
					<xs:documentation>地址组为IPV4或者IPV6</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vsiName">
				<xs:annotation>
					<xs:documentation>vsi的名字</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="31"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ssmAcl">
				<xs:annotation>
					<xs:documentation>配置ssm-policy ACL Name or Number</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="version" default="2">
				<xs:annotation>
					<xs:documentation>配置的igmp版本号</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="3"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ssmapEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>接口SSMapping 使能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="fwdMode" type="l2mcFwdMode" default="ip">
				<xs:annotation>
					<xs:documentation>转发模式： 1:ip, 2:mac</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="snoopingEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>VLAN或VSI的igmp snooping使能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="groupAcl">
				<xs:annotation>
					<xs:documentation>igmp snooping组策略  ACL Name or Number</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="groupAclGMPVer" default="0">
				<xs:annotation>
					<xs:documentation>igmp snooping 组策略指定的IGMP版本号</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="3"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="queryRspInterval" default="10">
				<xs:annotation>
					<xs:documentation>查询最大响应时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="25"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="immediatelyLeave" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>是否配置立刻离开组</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="immLeaveAcl">
				<xs:annotation>
					<xs:documentation>配置立刻离开组ACL Name or Number</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="proxyEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>VLAN或VSI的igmp proxy使能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="asmSsmMode" type="l2mcSnpMode" default="asm-ssm">
				<xs:annotation>
					<xs:documentation>当前的模式： asm-only、ssm-only、asm-ssm
                    </xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipPolicyAcl">
				<xs:annotation>
					<xs:documentation>用来控制对IGMP Report报文中目的IP和源地址进行过滤</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
            <xs:element name="queryIpPolicyAcl">
                <xs:annotation>
                    <xs:documentation>用来控制对IGMP Query报文中IP进行过滤</xs:documentation>
                        <xs:appinfo>
                            <access>readCreate</access>
                        </xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
			<xs:element name="querierElection" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>配置VLAN或VSI下的查询器选举功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="proxyRouterProtocalPass" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>默认情况下使能igmp Proxy后会终结路由协议报文</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="querierEnable" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>VLAN或VSI内使能查询器的功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="queryInterval" default="60">
				<xs:annotation>
					<xs:documentation/>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="65535"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="requireRouterAlert" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>是否需要RouterAlert</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sendRouterAlert" type="xs:boolean" default="true">
				<xs:annotation>
					<xs:documentation>是否发送RouterAlert</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="robustness" default="2">
				<xs:annotation>
					<xs:documentation>鲁棒稳定系数</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="5"/>
						<xs:minInclusive value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="routerAgingTime" default="180">
				<xs:annotation>
					<xs:documentation>路由端口的存活时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="1000"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="routerPortLearning" type="xs:boolean" default="true">
				<xs:annotation>
					<xs:documentation>使能VLAN的路由器接口动态学习功能</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="querySuppressTime" default="10">
				<xs:annotation>
					<xs:documentation>用来配置IGMP Query报文抑制的时间</xs:documentation>
					<xs:appinfo>
                    </xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="300"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lastMemQI" default="1">
				<xs:annotation>
					<xs:documentation>特定组查询的周期性时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="5"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="stGrpSupJoin" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>如果S9300上游三层组播设备为其它厂商设备，并且在此三层设备的接口上配置了静态组播组之后，则不允许用户以动态的方式加入或者退出组播组，此时需要在S9300设备上配置禁止向三层组播设备发送的Report和Leave报文。</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="reportSuppress" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>对下游设备发送的report和leave报文进行抑制</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="soureLifeTime" default="210">
				<xs:annotation>
					<xs:documentation>路由端口的存活时间</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="1000"/>
						<xs:minInclusive value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="l2McVsiPwPorts" type="l2mc_l2McVsiPwPorts_type"/>
			<xs:element name="l2McVsiPwGrpPorts" type="l2mc_l2McVsiPwGrpPorts_type"/>
			<xs:element name="l2McVsiAcStRtPorts" type="l2mc_l2McVsiAcGrpPorts_type"/>
			<xs:element name="l2McVsiIfGroupPolicys" type="l2mc_l2McVsiIfGroupPolicys_type"/>
			<xs:element name="l2McVsiStaticGroups" type="l2mc_l2McVsiStaticGroups_type"/>
			<xs:element name="l2McVsiDyn" type="l2mc_l2McVsiDyn_type"/>
			<xs:element name="l2McVsiQuerier" type="l2mc_l2McVsiQuerier_type"/>
			<xs:element name="l2McVsiRoutePorts" type="l2mc_l2McVsiRoutePorts_type"/>
			<xs:element name="l2McACPorts" type="l2mc_l2McACPorts_type"/>
			<xs:element name="l2McVsiPortInfoEntrys" type="l2mc_l2McVsiPortInfoEntrys_type"/>
			<xs:element name="l2McVsiPwGroups" type="l2mc_l2McVsiPwGroups_type"/>
			<xs:element name="l2McVsiStatis" type="l2mc_l2McVsiStatis_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiOthers_type">
		<xs:sequence>
			<xs:element name="l2McVsiOther" type="l2mc_l2McVsiOther_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiOther_type">
		<xs:annotation>
			<xs:documentation>VLAN内的产品的配置信息</xs:documentation>
			<xs:appinfo>
				<access>readOnly</access>
				<category>混合</category>
				<status>current</status>
				<version>1.0</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="addrFamily" type="l2mcAfType">
				<xs:annotation>
					<xs:documentation>地址组为IPV4或者IPV6</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vsiName">
				<xs:annotation>
					<xs:documentation>vsi的名字</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="31"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="mcTrunkLbEnable" type="xs:boolean" default="0">
				<xs:annotation>
					<xs:documentation>组播trunk负载分担使能，仅在igmp-snooping不使能时使用</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dropUnknown" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>配置组播未知流的转发方式为丢弃</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiPwPorts_type">
		<xs:sequence>
			<xs:element name="l2McVsiPwPort" type="l2mc_l2McVsiPwPort_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiPwPort_type">
		<xs:annotation>
			<xs:documentation>Vsi内igmp snooping PW类型接口配置</xs:documentation>
			<xs:appinfo>
				<access>readCreate</access>
				<key>remotePeerAddr;negotiationVcId</key>
				<status>current</status>
				<version>1.0</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="negotiationVcId">
				<xs:annotation>
					<xs:documentation>配置的虚电路的唯一标识，一般用于两端VSI ID不同但要求互通的情况。</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="4294967295"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="remotePeerAddr" type="ipAddress">
				<xs:annotation>
					<xs:documentation>对端Peer的IP地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiPwGrpPorts_type">
		<xs:sequence>
			<xs:element name="l2McVsiPwGrpPort" type="l2mc_l2McVsiPwGrpPort_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiPwGrpPort_type">
		<xs:annotation>
			<xs:documentation>Vsi内igmp snooping PW类型接口配置</xs:documentation>
			<xs:appinfo>
				<access>readCreate</access>
				<key>remotePeerAddr;negotiationVcId</key>
				<status>current</status>
				<version>1.0</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="negotiationVcId">
				<xs:annotation>
					<xs:documentation>配置的虚电路的唯一标识，一般用于两端VSI ID不同但要求互通的情况。</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:maxInclusive value="4294967295"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="remotePeerAddr" type="ipAddress">
				<xs:annotation>
					<xs:documentation>对端Peer的IP地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="l2McVsiPwStaticGroups" type="l2mc_l2McVsiPwStaticGroups_type"/>
		</xs:choice>
		<xs:attributeGroup ref="objectAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiPwStaticGroups_type">
		<xs:sequence>
			<xs:element name="l2McVsiPwStaticGroup" type="l2mc_l2McVsiPwStaticGroup_type" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="l2mc_l2McVsiPwStaticGroup_type">
		<xs:annotation>
			<xs:documentation>VSI视图内PW类型igmp snooping静态组配置</xs:documentation>
			<xs:appinfo>
				<access>readCreate</access>
				<key>groupAddress;sourceAddress;remotePeerAddr;negotiationVcId</key>
				<status>current</status>
				<version>1.0</version>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="groupAddress" type="ipAddress">
				<xs:annotation>
					<xs:documentation>配置的静态组地址</xs:documentation>
					<xs:appinfo>
						<access>readCreate</access>
						<key>true</key>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sourceAddress" type="ipAddress">
				<xs:annotation>
